{
  "presets": ["next/babel"],
  "plugins": [
    ["import", { "libraryName": "antd", "style": true }, "antd"],
    ["import", { "libraryName": "antd-mobile", "style": true }, "antd-mobile"]
  ],
  "env": {
    "development": {
      "plugins": []
